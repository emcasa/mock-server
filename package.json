{
  "name": "@emcasa/mock-server",
  "version": "0.2.6",
  "author": "EmCasa <dev@emcasa.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "bin": {
    "emcasa-mock-server": "lib/bin/run.js"
  },
  "scripts": {
    "start": "nodemon src/bin/run.js",
    "build": "babel -d lib src",
    "update-schema": "cd backend && mix absinthe.schema.json --schema ReWeb.Schema --pretty && mv schema.json ../schema.json",
    "prepare-backend": "git submodule init && (cd backend && cp -n config/dev.secret-example.exs config/dev.secret.exs || mix deps.get)",
    "prepare": "yarn prepare-backend && yarn build"
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.0.4",
    "body-parser": "^1.18.3",
    "buffer": "^5.2.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "fs": "^0.0.1-security",
    "lodash.flow": "^3.5.0",
    "lodash.mapvalues": "^4.6.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "slugify": "^1.3.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.4",
    "@babel/core": "^7.0.0-rc.4",
    "@babel/node": "^7.0.0-rc.4",
    "@babel/preset-env": "^7.0.0-rc.4",
    "get-graphql-schema": "^2.1.1",
    "nodemon": "^1.18.3"
  },
  "peerDependencies": {
    "graphql": "^0.13.2",
    "graphql-tools": "^3.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
